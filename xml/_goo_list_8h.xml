        <name>deleteGooList</name>
        <param><defname>list</defname></param>
        <param><defname>T</defname></param>
        <initializer>do {                                              \
    <ref refid="class_goo_list" kindref="compound">GooList</ref> *_list = (list);                          \
    {                                               \
      int _i;                                       \
      for (_i = 0; _i &lt; _list-&gt;getLength(); ++_i) { \
        delete (T*)_list-&gt;get(_i);                  \
      }                                             \
      delete _list;                                 \
    }                                               \
  } while (0)</initializer>
